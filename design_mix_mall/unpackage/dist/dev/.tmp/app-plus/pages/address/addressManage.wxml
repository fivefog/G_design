<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','real_name','$event',[]],['addressData']]]]]}}" value="{{addressData.real_name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['addressData']]]]]}}" value="{{addressData.phone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><text data-event-opts="{{[['tap',[['openAddres',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.province+" "+addressData.city+" "+addressData.district+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><input class="input" type="text" placeholder="详细地址: 如道路、小区、楼栋号、单元室等" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','detail','$event',[]],['addressData']]]]]}}" value="{{addressData.detail}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{$root.m0}}" color="#F47E0D" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button><simple-address class="vue-ref" vue-id="1" pickerValueDefault="{{cityPickerValueDefault}}" themeColor="#F47E0D" data-ref="simpleAddress" data-event-opts="{{[['^onConfirm',[['onConfirm']]]]}}" bind:onConfirm="__e" bind:__l="__l"></simple-address></view>